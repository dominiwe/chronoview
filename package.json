{
	"name": "planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && touch build/.nojekyll",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@event-calendar/core": "^0.15.0",
		"@event-calendar/day-grid": "^0.15.0",
		"@event-calendar/interaction": "^0.15.0",
		"@event-calendar/list": "^0.15.0",
		"@event-calendar/resource-time-grid": "^0.15.0",
		"@event-calendar/time-grid": "^0.15.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-static": "^1.0.1",
		"@sveltejs/kit": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"cssnano": "^5.1.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"patch-package": "^6.5.1",
		"postcss": "^8.4.20",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.1",
		"prettier-plugin-css-order": "^1.3.0",
		"prettier-plugin-jsdoc": "^0.4.2",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
